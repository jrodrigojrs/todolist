{
 "expo": {
  "name": "todolist",
  "slug": "todolist",
  "version": "1.0.0",
  "orientation": "portrait",
  "icon": "./assets/images/icon.png",
  "scheme": "myapp",
  "userInterfaceStyle": "automatic",
  "newArchEnabled": true,
  "splash": {
   "image": "./assets/images/splash-icon.png",
   "resizeMode": "contain",
   "backgroundColor": "#CBFBF1"
  },
  "ios": {
   "supportsTablet": true
  },
  "android": {
   "adaptiveIcon": {
    "foregroundImage": "./assets/images/adaptive-icon.png",
    "backgroundColor": "#CBFBF1"
   },
   "package": "com.jrod_br.todolist"
  },
  "web": {
   "bundler": "metro",
   "output": "static",
   "favicon": "./assets/images/favicon.png"
  },
  "plugins": ["expo-router"],
  "experiments": {
   "typedRoutes": true
  },
  "extra": {
   "router": {
    "origin": false
   },
   "eas": {
    "projectId": "9ff94eb2-9a66-4538-8eca-201875f81f55"
   }
  },
  "owner": "jrod_br"
 }
}
